#header-holder
  #header(class='row')
    #logo(class='col-xs-3')
      //#logo-pix Pix-
      //#logo-e E
      img#logo-icon(src='assets/images/pixeIcon.svg')
      #logo-lander L A N D E R
    #second-bar(class='col-xs-9 row' style="padding:0;margin:0;")
      #user-greeting(class='col-sm-4')
        div Welcome Back
      #user-bar(class='col-sm-8')
        #user-name {{name}}
        #user-icon-holder
          img#user-icon(src='assets/images/radcliffe.jpg')
        #edit-bar()
          a#create-note(ui-sref=".submit" class="fa fa-pencil" ng-click="createNote()")
          a(ui-sref=".login" class="" ) login

  #menu-button(class='menu-button-default-animation' on='false' data-menu-Button)
    h1
      span(class='fa fa-book')
